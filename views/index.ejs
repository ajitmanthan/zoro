      $(".l27").addClass("animate__animated  animate__bounceInRight ")
      $(".tabss").addClass("animate__animated animate__bounceInLeft ")
      }
      else if((wscall>550)&&(wscall<860)){
      $(".l25").addClass("animate__animated animate__bounceInLeft")
      $(".l26").addClass("animate__animated animate__bounceInRight")}

      else if((wscall>860)&&(wscall<1350)){
      $(".l25").addClass("animate__animated animate__bounceInRight")
