  $(window).scroll(function(){
    var wscall=$(document).scrollTop()
    console.log(wscall)
 
  
   if((wscall>100)&&(wscall<550)){
      $(".l277").addClass("animate__animated animate__bounceInLeft")
      $(".lala14").addClass("animate__animated animate__bounceInLeft")
      $(".l25").addClass("animate__animated animate__bounceInLeft")
